{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "if-env NODE_ENV=production ?? npm run start:prod || npm run start:dev",
    "start:dev": "nodenv -f .",
    "start:prod": "node .",
    "test": "jest",
    "seed": "npx sequelize-cli db:seed:all",
    "unseed": "npx sequelize-cli db:seed:undo:all",
    "seed-reset": "npm run unseed && npm run seed ",
    "dev": "node app.js",
    "prod": "node app.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "exif": "^0.6.0",
    "express": "~4.16.1",
    "express-jwt": "^6.1.1",
    "express-openapi": "^10.1.0",
    "fs": "0.0.1-security",
    "helmet": "^5.0.2",
    "http-errors": "~1.6.3",
    "jest": "^27.5.1",
    "json-refs": "^3.0.15",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.4",
    "nodemailer": "^6.7.5",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "pug": "2.0.0-beta11",
    "sequelize": "^6.17.0",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui": "^4.6.1",
    "swagger-ui-dist": "^4.6.1",
    "swagger-ui-express": "^4.3.0",
    "validator": "^13.7.0"
  }
}
